<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>AdminLoginServ</servlet-name>
        <servlet-class>pack1.AdminLoginServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AuditLoginServ</servlet-name>
        <servlet-class>pack1.AuditLoginServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangeAdminpswdServ</servlet-name>
        <servlet-class>pack1.ChangeAdminpswdServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteUserServ</servlet-name>
        <servlet-class>pack1.DeleteUserServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LaunchServ</servlet-name>
        <servlet-class>pack1.LaunchServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServ</servlet-name>
        <servlet-class>pack1.LoginServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignupServ</servlet-name>
        <servlet-class>pack1.SignupServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TpaRegisterServ</servlet-name>
        <servlet-class>pack1.TpaRegisterServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminLoginServ</servlet-name>
        <url-pattern>/AdminLoginServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AuditLoginServ</servlet-name>
        <url-pattern>/AuditLoginServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangeAdminpswdServ</servlet-name>
        <url-pattern>/ChangeAdminpswdServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteUserServ</servlet-name>
        <url-pattern>/DeleteUserServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LaunchServ</servlet-name>
        <url-pattern>/LaunchServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServ</servlet-name>
        <url-pattern>/LoginServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignupServ</servlet-name>
        <url-pattern>/SignupServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TpaRegisterServ</servlet-name>
        <url-pattern>/TpaRegisterServ</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
